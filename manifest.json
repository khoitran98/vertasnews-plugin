{
	"name":"VertasNewsPlugin",
	"description":"desc",
	"version":"1.0",
	
	"manifest_version":2,
	
	"browser_action":{
		"default_icon":"favicon.png",
		"default_popup":"popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"permissions":[
		"tabs",
		"activeTab",
		"storage"
	]
}